vel_55_atp_2000 = np.array([0.6642,0.2022,0.1516,0.1356,0.0934,0.0702,0.0655,0.0745,0.0608,0.0582,0.0567,0.0607])

plt.figure(figsize=(10,6))
plt.suptitle("5-Kinesin, 5-Dynein")
plt.subplot(2,3,1).title.set_text('G')
plt.plot(cal,vel_55_atp_500, '--o', color = 'black', markerfacecolor='none')
